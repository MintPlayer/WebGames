{
  "compilerOptions": {
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": false,
    "sourceMap": true,
    "target": "ES6",
    "outDir": "wwwroot/js",
    "rootDir": "Scripts"
  },
  "include": [
    "Scripts/**/*"
  ],
  "exclude": [
    "node_modules"
  ],
  "compileOnSave": true
}
